source 'https://github.com/CocoaPods/Specs.git'
target 'RTC' do
  platform :ios, '10.0'
  pod 'GoogleWebRTC'
  pod 'Firebase/Core'
  pod 'Firebase/Analytics'
  pod 'Firebase/Database'
  pod 'Firebase/Firestore'
  pod 'Firebase/Auth'
  pod 'Firebase/Storage'
  pod 'GoogleWebRTC'
  pod 'Starscream'
end

target 'RTCTests' do
  platform :ios, '10.0'
  pod 'GoogleWebRTC'
  pod 'Firebase/Core'
  pod 'Firebase/Analytics'
  pod 'Firebase/Database'
  pod 'Firebase/Firestore'
  pod 'Firebase/Auth'
  pod 'GoogleWebRTC', '~> 1.1'
  pod 'Starscream', '~> 3.0'
end

